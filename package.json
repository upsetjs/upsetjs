{
  "name": "root",
  "version": "0.5.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "homepage": "https://github.com/upsetjs/upsetjs",
  "bugs": {
    "url": "https://github.com/upsetjs/upsetjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/upsetjs.git"
  },
  "scripts": {
    "lint": "npm run prettier:check && yarn workspaces foreach -v -p --topological-dev run lint",
    "fix": "npm run prettier:write && yarn workspaces foreach -v --topological-dev run fix",
    "test": "yarn workspaces foreach -v -p --topological-dev run test",
    "build": "yarn workspaces foreach -v -p --topological-dev run build",
    "build:preapp": "yarn workspaces foreach -v -p --topological-dev --include @upsetjs/model --include @upsetjs/math --include @upsetjs/bundle --include @upsetjs/react --include @upsetjs/addons run build",
    "docs": "yarn workspaces foreach -v -p run docs",
    "storybook": "yarn workspace @upsetjs/react run storybook",
    "app": "yarn workspace @upsetjs/app run start",
    "prettier": "prettier .eslintrc.js .prettierrc *.json .yarnrc_patch.yml \".github/**\"",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check"
  },
  "resolutions": {
    "tsdx/rollup-plugin-typescript2": "^0.26.0",
    "@typescript-eslint/typescript-estree": "^2.26.0",
    "jss-plugin-rule-value-function": "10.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@typescript-eslint/typescript-estree": "^2.26.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.20",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "dependenciesMeta": {
    "@storybook/*": {
      "unplugged": true
    },
    "@storybook/components": {
      "unplugged": true
    },
    "@storybook/core": {
      "unplugged": true
    }
  }
}
